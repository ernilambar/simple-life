{
	"env": {
		"browser": true,
		"node": true
	},
	"extends": [ "plugin:@wordpress/eslint-plugin/es5" ],
	"parserOptions": {
		"requireConfigFile": false
	},
	"globals": {
		"ajaxurl": true,
		"wp": true,
		"jQuery": true,
		"simpleLifeScreenReaderText": true
	},
	"rules": {
		"vars-on-top": "warn",
		"no-console": "error"
	}
}
